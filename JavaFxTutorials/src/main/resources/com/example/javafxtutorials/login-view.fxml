      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.javafxtutorials.LoginView">
    <HBox alignment="CENTER">
        <Label text="nhap ten"/>
        <TextField fx:id="name"/>
    </HBox>
    <HBox>
        <Label text="nhap mat khau"/>
        <TextField fx:id="pass"/>
    </HBox>
    <HBox>
        <Button text="Login" onAction="#login"/>
    </HBox>

</VBox>




